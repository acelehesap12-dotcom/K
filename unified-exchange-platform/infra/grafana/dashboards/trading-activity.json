{
  "dashboard": {
    "title": "KK99 Exchange - Trading Activity",
    "tags": ["kk99", "trading", "orders"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Orders Created (per second)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(orders_created_total[1m])",
            "legendFormat": "{{symbol}} {{side}}"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Trades Executed (per second)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(trades_executed_total[1m])",
            "legendFormat": "{{symbol}}"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
      },
      {
        "id": 3,
        "title": "Order Processing Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(order_processing_time_ms_bucket[5m]))",
            "legendFormat": "{{symbol}} p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(order_processing_time_ms_bucket[5m]))",
            "legendFormat": "{{symbol}} p99"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "yaxes": [
          {
            "format": "ms",
            "label": "Latency"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [50],
                "type": "gt"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "message": "Order processing latency exceeds 50ms",
          "name": "High Order Latency",
          "noDataState": "no_data"
        }
      },
      {
        "id": 4,
        "title": "Trade Execution Latency (microseconds)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(trade_execution_latency_us_bucket[5m]))",
            "legendFormat": "{{symbol}} p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(trade_execution_latency_us_bucket[5m]))",
            "legendFormat": "{{symbol}} p99"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "yaxes": [
          {
            "format": "Âµs",
            "label": "Latency"
          }
        ]
      },
      {
        "id": 5,
        "title": "Partial Fills",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(orders_filled_partial_total[5m])",
            "legendFormat": "{{symbol}}"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 }
      },
      {
        "id": 6,
        "title": "Total Orders (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(orders_created_total[24h]))"
          }
        ],
        "gridPos": { "h": 4, "w": 6, "x": 12, "y": 16 }
      },
      {
        "id": 7,
        "title": "Total Trades (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(trades_executed_total[24h]))"
          }
        ],
        "gridPos": { "h": 4, "w": 6, "x": 18, "y": 16 }
      },
      {
        "id": 8,
        "title": "Trading Volume by Symbol",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (symbol) (trades_executed_total)",
            "legendFormat": "{{symbol}}"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 }
      }
    ],
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}
